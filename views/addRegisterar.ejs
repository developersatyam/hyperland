    <div class="container">
        <br>
        <h1>Add a Registerar</h1>
        <form name="addParticipant" method="POST" action="/addRegisterar">

            <div class="row">
                <div class="col-lg-6 col-sm-12 form-group">
                    <label for="ID">ID</label>
                    <input type="text" class="form-control" name="rid" id="rid">
                </div>
            </div>

            <div class="row">                
                <div class="col-lg-6 col-sm-12 form-group">
                    <label for="bname">Registerar Name </label>
                    <input type="text" class="form-control" name="rname" id="rname">
                </div>
            </div>

            <div class="row">                
                <div class="col-lg-6 col-sm-12 form-group">
                    <label for="bname">Designation </label>
                    <input type="text" class="form-control" name="desg" id="desg">
                </div>
            </div>

            <div class="row">
                <div class="col-lg-6 col-sm-12 form-group">
                    <label for="balance">Bank Balance</label>
                    <input type="number" class="form-control" name="bankbal" id="bankbal">
                </div>
            </div>
            
            
            <br>
            <button type="submit" class="btn btn-primary">Add Registerar</button>
            <br>
            <br>
        </form>
    </div>


